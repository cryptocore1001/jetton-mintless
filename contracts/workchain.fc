#include "stdlib.fc";
#include "op-codes.fc";

const WORKCHAIN = BASECHAIN;

int is_same_workchain(slice addr) inline {
    (int wc, _) = parse_std_addr(addr);
    return wc == WORKCHAIN;
}

() check_same_workchain(slice addr) impure {
  throw_unless(error::wrong_workchain, is_same_workchain(addr));
}